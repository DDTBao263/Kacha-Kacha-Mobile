{
  "expo": {
    "name": "Kacha-Kacha",
    "scheme": "your-app-scheme",
    "slug": "Kacha-Kacha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hotpoticon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Kacha-Kacha to use Face ID for authentication."
      },
      "bundleIdentifier": "com.tydy234.KachaKacha"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hotpoticon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tydy234.KachaKacha",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Kacha-Kacha to use Face ID."
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "26c18035-e065-4803-ad2a-cf5c6b5de3fa"
      }
    }
  }
}
